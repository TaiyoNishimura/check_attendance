<div class="main set">
  <div class="container">
    <div class="form-heading">講義を追加する</div>
    <div class="form">
      <div class="form-body">
        <%= form_tag("/create") do %>
          <p>科目名</p>
          <input name="subject_name">
          <p>曜日</p>
          <input name="day_of_the_week" >
          <p>時限</p>
          <input name="period">
          <p>最初の授業の日付</p>
          <input name="first_date">
          <input type="submit" value="追加する">
        <% end %>
      </div>
    </div>
  </div>
</div>